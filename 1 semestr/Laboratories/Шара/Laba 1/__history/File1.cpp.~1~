//---------------------------------------------------------------------------

#pragma hdrstop

#include <tchar.h>
#include <iostream.h>
#include <conio.h>
#include <math.h>
using namespace std;

//---------------------------------------------------------------------------

int _tmain(int argc, _TCHAR* argv[])
{
	double A,B,Y,x,z,p,C,K,D;
		cout<<"Vvedite: x=";
	cin>>x;
		cout<<"z=";
	cin>>z;
		cout<<"p=";
	cin>>p;
		cout<<"K=";
	cin>>K;
		cout<<"C=";
	cin>>C;
		cout<<"D=";
	cin>>D;

	A=sin(x)-z;
	B=fabs(p-x);
	Y=pow((A+B),2)-(K/(C*D));
	cout<<"Otvet: Y="<<Y;

getch();
return 0;
}
//---------------------------------------------------------------------------
